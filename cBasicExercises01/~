#include<stdio.h>
main()
{
   int a,b,sum,sub,mul;                           
   scanf("%d",&a);                       
   scanf("%d",&b);                       
   sum=a+b;                               
   sub=a-b;
   mul=a*b;
   printf("%d",sum); 
   printf(" %d",sub);         
   printf(" %d\n",mul); 
   printf("%d\n",sum); 
   printf("%d\n",sub);        
   printf("%d\n",mul); 
}

